<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
        <suite name="TestingChrome" parallel="tests" thread-count="2" verbose="5">

    <parameter name="browserName" value="chrome"/>
    <test name="SingleTestSuiteChrome">
        <groups>
            <run>
                <include name="functest"/>
            </run>
        </groups>
        <classes>
            <class name="framework.YahooTest"/>
        </classes>
    </test>
    <test name="SingleTestSuiteChrome1">
        <groups>
            <run>
                <include name="functest1"/>
            </run>
        </groups>
        <classes>
            <class name="framework.YahooTest" />
        </classes>
    </test>

</suite>